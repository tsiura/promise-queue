    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Yevhenii Tsiura",
            "email": "tsura.evgeniy@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.3",
        "react/promise": "^3.2",
        "psr/log": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.0",
        "squizlabs/php_codesniffer": "^3.12",
        "vimeo/psalm": "^6.10",
        "phpstan/phpstan": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Zeran\\PromiseQueue\\": "src/"
        }
